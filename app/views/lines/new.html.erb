<% @page_title = "New Product Line" %>

<%= link_to("<< Back to List", {:action => 'index'}, :class => 'back-link') %>

<div class="lines new">
  <h2>Create Product Line</h2>

  <%= form_for(:line, :url => {:action => 'create'}) do |f| %>

    <%= render(:partial => "form", :locals => {:f => f}) %>

    <div class="form-buttons">
      <%= submit_tag("Create Product Line") %>
    </div>

  <% end %>
</div>
